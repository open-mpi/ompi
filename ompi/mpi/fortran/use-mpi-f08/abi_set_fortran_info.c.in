PROTOTYPE VOID abi_set_fortran_info(INFO info)
{
    int c_ierr;
    MPI_Info c_info = PMPI_Info_f2c(*info);

    c_ierr = @INNER_CALL@(c_info);
    if (NULL != ierr) *ierr = OMPI_INT_2_FINT(c_ierr);
}
