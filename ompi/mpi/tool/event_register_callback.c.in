#include "ompi_config.h"

#include "ompi/mpi/tool/mpit-internal.h"

PROTOTYPE ERROR_CLASS event_register_callback (EVENT_REGISTRATION event_registration,
                                               CB_SAFETY cb_safety, INFO info, VOID user_data,
                                               EVENT_CB_FUNCTION event_cb_function)
{
    if (!mpit_is_initialized ()) {
        return MPI_T_ERR_NOT_INITIALIZED;
    }

    return MPI_T_ERR_INVALID_HANDLE;
}
