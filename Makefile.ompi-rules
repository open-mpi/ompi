.1in.1:
	@ echo "  SED      $@"
	@ sed -e 's/#PACKAGE_NAME#/@PACKAGE_NAME@/g' \
	  -e 's/#PACKAGE_VERSION#/@PACKAGE_VERSION@/g' \
	  -e 's/#OMPI_DATE#/@OMPI_RELEASE_DATE@/g' \
	  -e 's/#OPAL_DATE#/@OMPI_RELEASE_DATE@/g' \
	  -e 's/#ORTE_DATE#/@OMPI_RELEASE_DATE@/g' \
	  > $@ < $<

